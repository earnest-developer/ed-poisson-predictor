{
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.pythonPath": ".venv/bin/python",
    "protoc": {
        "path": "${workspaceRoot}/.venv/lib/python3.7/site-packages/grpc_tools/protoc.py",
        "compile_on_save": true,
        "options": [
            "--proto_path=./protos",
            "--python_out=${workspaceRoot}",
            "--grpc_python_out=${workspaceRoot}"
        ]
    }
}